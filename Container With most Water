class Solution {
public:
    int maxArea(vector<int>& height) {
        int left=0;
        int right=height.size()-1;
        int maxAreas=0;
        int res=0;
        while(left<right){
            int h=min(height[left],height[right]);
            int w=right-left;
            res=h*w;
            if(height[left]<height[right])
            ++left;
            else
            --right;

             maxAreas=max(maxAreas,res);
        }
        return maxAreas;
    }
};
